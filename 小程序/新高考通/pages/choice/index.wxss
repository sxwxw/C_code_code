.head {
  padding: 10rpx 20rpx;
  margin-top: 50rpx;
}
.head h1 {
  font-size: 60rpx;
  font-weight: bold;
}
.head .head_tip {
  font-size: 30rpx;
  color: #aaa;
}
.content {
  width: 100%;
  margin-top: 100rpx;
}
.content .pro {
  width: 100%;
  height: 145rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
  position: relative;
  display: flex;
  flex-direction: column;
}
.content .pro .pro_cho {
  line-height: 100rpx;
  border-bottom: 1px solid #f5eeee;
}
.content .pro .pro_cho .cho_one,
.content .pro .pro_cho .cho_two {
  float: left;
}
.content .pro .pro_cho .cho_one span {
  color: red;
  margin-right: 10rpx;
}
.content .pro .pro_cho .cho_two {
  margin-left: 40rpx;
}
.content .pro .pro_cho .cho_three {
  float: right;
}
.content .pro .pro_tip {
  position: absolute;
  bottom: 0;
  font-size: 25rpx;
  color: orange;
}
.content .cate {
  width: 100%;
  height: 120rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
  position: relative;
  display: flex;
  flex-direction: column;
}
.content .cate .cate_cho {
  line-height: 100rpx;
  border-bottom: 1rpx solid #f5eeee;
}
.content .cate .cate_cho .cho_one,
.content .cate .cate_cho .cho_two {
  float: left;
}
.content .cate .cate_cho .cho_two {
  margin-left: 40rpx;
}
.content .cate .cate_cho .cho_one span {
  color: red;
  margin-right: 10rpx;
}
.content .cate .cate_cho .cho_three {
  float: right;
}
