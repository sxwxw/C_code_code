.content .hide_cont .fil {
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
  margin-top: 40rpx;
}
.content .hide_cont .fil .fil_cont .grade {
  display: flex;
}
.content .hide_cont .fil .fil_cont .grade .grade_one {
  font-size: 35rpx;
}
.content .hide_cont .fil .fil_cont .grade .grade_one span {
  color: red;
  margin-right: 10rpx;
}
.content .hide_cont .fil .fil_cont .grade .grade_two {
  position: relative;
  display: flex;
}
.content .hide_cont .fil .fil_cont .grade .grade_two input {
  width: 460rpx;
  margin-left: 40rpx;
  font-size: 35rpx;
}
.content .hide_cont .fil .fil_cont .grade .grade_two span {
  position: absolute;
  right: -40rpx;
}
.content .hide_cont .fil .fil_cont .rank {
  display: flex;
  margin-top: 20rpx;
}
.content .hide_cont .fil .fil_cont .rank .rank_one {
