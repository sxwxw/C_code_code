.btn {
  margin-top: 50rpx;
  margin-bottom: 100rpx;
}
.btn .btn_text {
  width: 100%;
  font-size: 20rpx;
  text-align: center;
}
.btn .btn_cont {
  width: 100%;
  display: flex;
  justify-content: center;
  margin-top: 10rpx;
}
.btn .btn_cont navigator {
  width: 90%;
  height: 80rpx;
  background-color: pink;
  text-align: center;
  line-height: 80rpx;
  border-radius: 10rpx;
}
.input-item {
  display: flex;
  width: 90%;
  height: 100rpx;
  line-height: 100rpx;
  align-items: center;
  padding: 0 10rpx;
  margin: 0 auto;
  border-bottom: 2rpx solid #efefef;
  font-size: 30rpx;
}
.input-item .input-label {
  width: 160rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
}
.input-item .input-label.defaule-text::before {
  content: '*';
  color: red;
  margin-right: 10rpx;
}
.input-item .input-text {
  flex: 1;
  height: 100rpx;
  line-height: 100rpx;
}
.input-item .input-last-text {
  width: 50rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: right;
  color: #cacaca;
}
